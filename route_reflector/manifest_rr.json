{
  "jit_all": true,
  "plugins": {
    "route_reflector": {
      "extra_mem": 4096,
      "shared_med": 0,
      "obj_code_list": {
        "import_route_rr": {
          "obj": "import_route_rr.o",
          "permissions": [
            "read",
            "write",
            "usr_ptr"
          ],
          "verifiers" : {
            "seahorn": "-DPROVERS -DPROVERS_SEAHORN",
            "t2": "-DPROVERS -DPROVERS_T2",
            "cbmc": "-DPROVERS -DPROVERS_CBMC"
          }
        },
        "export_route_rr": {
          "obj": "export_route_rr.o",
          "permissions": [
            "read",
            "write",
            "usr_ptr"
          ],
          "verifiers" : {
            "seahorn": "-DPROVERS -DPROVERS_SEAHORN",
            "t2": "-DPROVERS -DPROVERS_T2",
            "cbmc": "-DPROVERS -DPROVERS_CBMC"
          }
        },
        "encode_cluster_list": {
          "obj": "encode_cluster_list.o",
          "permissions": [
            "read",
            "write",
            "usr_ptr"
          ],
          "verifiers" : {
            "seahorn": "-DPROVERS -DPROVERS_SEAHORN",
            "t2": "-DPROVERS -DPROVERS_T2",
            "cbmc": "-DPROVERS -DPROVERS_CBMC"
          }
        },
        "encode_originator_id": {
          "obj": "encode_originator_id.o",
          "permissions": [
            "read",
            "write",
            "usr_ptr"
          ],
          "verifiers" : {
            "seahorn": "-DPROVERS -DPROVERS_SEAHORN",
            "t2": "-DPROVERS -DPROVERS_T2",
            "cbmc": "-DPROVERS -DPROVERS_CBMC"
          }
        },
        "decode_cluster_list": {
          "obj": "decode_cluster_list.o",
          "permissions": [
            "read",
            "write",
            "usr_ptr"
          ],
          "verifiers" : {
            "seahorn": "-DPROVERS -DPROVERS_SEAHORN",
            "t2": "-DPROVERS -DPROVERS_T2",
            "cbmc": "-DPROVERS -DPROVERS_CBMC"
          }
        },
        "decode_originator_id": {
          "obj": "decode_originator.o",
          "permissions": [
            "read",
            "write",
            "usr_ptr"
          ],
          "verifiers" : {
            "seahorn": "-DPROVERS -DPROVERS_SEAHORN",
            "t2": "-DPROVERS -DPROVERS_T2",
            "cbmc": "-DPROVERS -DPROVERS_CBMC"
          }
        }
      }
    }
  },

  "insertion_points": {
    "bgp_pre_inbound_filter": {
      "replace": {
        "0": "import_route_rr"
      }
    },
    "bgp_pre_outbound_filter": {
      "replace": {
        "0": "export_route_rr"
      }
    },
    "bgp_encode_attr": {
      "replace": {
        "0": "encode_cluster_list",
        "10": "encode_originator_id"
      }
    },
    "bgp_decode_attr": {
      "replace": {
        "0": "decode_cluster_list",
        "10": "decode_originator_id"
      }
    }
  }
}