{
  "jit_all": false,
  "plugins": {
    "monitoring_stuffs": {
      "extra_mem": 512,
      "shared_mem": 0,
      "obj_code_list": {
        "as_path_cnt": {
          "obj": "as_path_count.o",
          "jit": true,
          "verifiers" : {
            "seahorn": "-DPROVERS -DPROVERS_SEAHORN",
            "t2": "-DPROVERS -DPROVERS_T2",
            "cbmc": "-DPROVERS -DPROVERS_CBMC"
          }
        },
        "route_origin": {
          "obj": "route_origin_monitor.o",
          "verifiers" : {
            "seahorn": "-DPROVERS -DPROVERS_SEAHORN",
            "t2": "-DPROVERS -DPROVERS_T2",
            "cbmc": "-DPROVERS -DPROVERS_CBMC"
          }
        }
      }
    }
  },
  "insertion_points": {
    "bgp_pre_inbound_filter": {
      "replace": {
        "0": "route_origin",
        "10": "as_path_cnt"
      }
    }
  }
}